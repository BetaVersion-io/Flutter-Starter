{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "游릭 Development Flavor",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "development",
        "--dart-define=FLAVOR=development",
        "--dart-define=ENVIRONMENT=development"
      ]
    },
    {
      "name": "游리 Staging Flavor",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "staging",
        "--dart-define=FLAVOR=staging",
        "--dart-define=ENVIRONMENT=staging"
      ]
    },
    {
      "name": "游리 Staging (Windows Emulator)",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "staging",
        "--dart-define=FLAVOR=staging",
        "--dart-define=ENVIRONMENT=staging",
        "--target=lib/main.dart"
      ]
    },
    {
      "name": "游댯 Production Flavor",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "production",
        "--dart-define=FLAVOR=production",
        "--dart-define=ENVIRONMENT=production"
      ]
    },
    {
      "name": "游릭 Dev (Quick Run)",
      "request": "launch",
      "type": "dart",
      "args": [
        "--flavor",
        "development",
        "--dart-define=FLAVOR=development",
        "--dart-define=ENVIRONMENT=development"
      ]
    },
    {
      "name": "游댠 Staging (Profile)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "args": [
        "--flavor",
        "staging",
        "--dart-define=FLAVOR=staging",
        "--dart-define=ENVIRONMENT=staging"
      ]
    },
    {
      "name": "游깷 Dev (Web)",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define=FLAVOR=development",
        "--dart-define=ENVIRONMENT=development"
      ],
      "deviceId": "chrome"
    }
  ]
}
